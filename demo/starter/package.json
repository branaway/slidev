{
  "name": "slidev-demo",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "nodemon -w '../../packages/slidev/dist/*.mjs' --exec \"slidev ./slides.md --open=true --log=info --inspect\"",
    "dev2": "nodemon -w '../../packages/slidev/dist/*.js' --exec \"../../packages/slidev/bin/slidev.mjs ./slides.md --open=true --log=info --inspect --remote='zxc'\"",
    "dev-bun": "bun ../../packages/slidev/node/cli.ts ./slides.md --base starter --open=true --log=info --inspect ",
    "export": "slidev export",
    "export-notes": "slidev export-notes"
  },
  "devDependencies": {
    "@slidev/cli": "workspace:*",
    "@slidev/theme-default": "catalog:",
    "@slidev/theme-seriph": "catalog:",
    "nodemon": "catalog:",
    "vue": "catalog:"
  }
}
